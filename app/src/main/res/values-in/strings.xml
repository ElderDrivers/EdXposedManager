    <!-- Welcome screen / main sections -->
    <string name="welcome">Selamat datang di <b>Xposed Installer</b>!\nHarap memilih apa yang ingin kamu lakukan:</string>
    <string name="tabInstall">Framework</string>
    <string name="tabInstallDescription">Di sini kamu dapat mengatur Xposed Framework, yang diperlukan untuk setiap modul supaya bekerja.</string>
    <string name="tabModules">Modul</string>
    <string name="tabModulesDescription">Framework itu sendiri tidak menambah fungsi apapun. Kamu masih memerlukan modul. Di sini kamu dapat mengaktifkan modul setelah kamu memasangnya.</string>
    <string name="tabDownload">Unduh</string>
    <string name="tabDownloadDescription">Bagian ini memberimu akses ke repositori modul, jadi kamu dapat mencari dan mengunduh satu yang kamu suka dan menjaganya tetap terupdate.</string>
    <string name="welcome_framework_update_available">Sebuah pembaharuan untuk Xposed Framework tersedia (%s).</string>
    <string name="welcome_update_available">Satu atau lebih pembaharuan untuk modul tersedia.</string>
    <string name="tabSettings">Pengaturan</string>
    <string name="tabSettingsDescription">Pergi ke bagian ini untuk mengatur Xposed Installer dan Framework.</string>
    <string name="tabSupport">Dukungan</string>
    <string name="tabSupportDescription">Temukan bantuan untuk masalah yang berhubungan dengan Xposed. Kamu juga dapat menyumbang untuk berterima kasih kepada Xposed.</string>
    <string name="tabLogs">Catatan</string>
    <string name="tabLogsDescription">Menampilkan dan menyimpan/mengirim catatan Xposed Framework.</string>
    <string name="tabAbout">Tentang</string>
